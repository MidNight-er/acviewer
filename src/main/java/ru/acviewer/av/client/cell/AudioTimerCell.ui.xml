<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.ui-timer {
		display: inline;
		position: relative;
		margin-left: 10px;
		margin-right: 32px;
		top: 1px;
	}
	.timer {
		position: absolute;
	}
	</ui:style>

	<ui:with type="java.lang.String" field="elementId"></ui:with>
	<ui:with type="java.lang.String" field="timer"></ui:with>

	<div class="{style.ui-timer}">
		<span id="{elementId}" class="{style.timer}">
			<ui:text from="{timer}" />
		</span>
	</div>
</ui:UiBinder>