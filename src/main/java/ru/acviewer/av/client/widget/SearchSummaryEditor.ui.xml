<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:bd="urn:import:com.github.gwtbootstrap.datetimepicker.client.ui">
	<ui:style src="Search.css">
	
	</ui:style>
	
	<g:HTMLPanel>
		<b:Container>
			<b:Form type="INLINE">
				<b:ControlGroup>
					<b:Controls>
 						<bd:DateTimeBox ui:field="dateTimeStart"
							format="dd.mm.yyyy hh:ii" autoClose="true"
							pickerPosition="BOTTOM_LEFT"
							addStyleNames="input-medium {style.acv-input}" />

						<bd:DateTimeBox ui:field="dateTimeEnd"
							format="dd.mm.yyyy hh:ii" autoClose="true"
							pickerPosition="BOTTOM_LEFT"
							addStyleNames="input-medium {style.acv-input}" />
					</b:Controls>
				</b:ControlGroup>
			</b:Form>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder> 